{
  "name": "nonagon",
  "version": "0.1.3",
  "private": true,
  "description": "Nonagon is a single page React/Redux application whose purpose is the creation of Spotify playlists based upon user's personal tastes",
  "keywords": [
    "javascript",
    "react",
    "redux",
    "jest",
    "es6",
    "spotify"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/bitfella/nonagon"
  },
  "license": "MIT",
  "author": "bitfella",
  "main": "src/index.js",
  "scripts": {
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "start": "react-scripts start",
    "test": "react-scripts test --transformIgnorePatterns \"node_modules/(?!lodash-es)/\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged --allow-empty"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "dependencies": {
    "@sentry/browser": "^5.18.0",
    "connected-react-router": "^6.7.0",
    "lodash-es": "^4.17.15",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-redux": "^7.2.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.0",
    "react-slider": "^1.0.3",
    "react-switch": "^5.0.1",
    "redux": "^4.0.5",
    "redux-api-middleware": "^3.2.0",
    "redux-thunk": "^2.3.0",
    "styled-components": "^5.0.1",
    "styled-normalize": "^8.0.7",
    "typeface-montserrat": "^0.0.75"
  },
  "devDependencies": {
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.4",
    "husky": "^4.2.5",
    "jest-canvas-mock": "^2.2.0",
    "jest-fetch-mock": "^3.0.1",
    "jest-localstorage-mock": "^2.4.0",
    "lint-staged": "^10.2.11",
    "node-fetch": "^2.6.0",
    "prettier": "^1.19.1",
    "redux-mock-store": "^1.5.4"
  },
  "engines": {
    "node": ">=12.0.0",
    "npm": ">=6.9.0",
    "yarn": ">=1.22.0"
  },
  "babelMacros": {
    "styledComponents": {}
  }
}